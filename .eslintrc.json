{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "react/function-component-definition": 0,
        "jsx-quotes": 0,
        "react/jsx-no-constructed-context-values": 0,
        "react/jsx-wrap-multilines": 0
    }
}
